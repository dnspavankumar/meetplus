        }
      ]
    },
    {
      "name": "Schedule Meeting",
      "short_name": "Schedule",
      "description": "Schedule a meeting for later",
      "url": "/?action=schedule-meeting",
      "icons": [
        {
          "src": "/icons/schedule.svg",
          "sizes": "96x96",
          "type": "image/svg+xml"
        }
      ]
    },
    {
      "name": "Personal Room",
      "short_name": "Personal",
